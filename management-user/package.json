{
  "name": "management-user",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "node ./dist/server.js",
    "dev": "npx tsnd --respawn --transpile-only --exit-child src/server.ts",
    "build": "rm -rf ./dist && npx tsc",
    "seed": "ts-node ./src/seed/seed_payment_method.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "generate-model-prisma": "npx prisma generate",
    "migration": "npx prisma migrate deploy",
    "build-non-migrate": "npm run generate-model-prisma && npm run build && npm run start",
    "build-migrate": "npm run migration && npm run build && npm run start",
    "test": "npx  tap --node-arg=--require=ts-node/register"
  },
  "keywords": [
    "fastify",
    "postgre",
    "node.js",
    "typescript",
    "prisma",
    "jsonwebtoken"
  ],
  "author": "Muhammad Ridho Hansyah",
  "license": "ISC",
  "devDependencies": {
    "@types/amqplib": "^0.10.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/fastify-jwt": "^0.8.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "pino-pretty": "^11.0.0",
    "prettier": "^3.2.5",
    "prisma": "^5.13.0",
    "supertest": "^7.0.0",
    "tap": "^18.7.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@prisma/client": "^5.13.0",
    "@supabase/supabase-js": "^2.42.7",
    "amqplib": "^0.10.4",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "fastify": "^4.26.2",
    "fastify-jwt": "^4.2.0",
    "fastify-zod": "^1.4.0",
    "zod-to-json-schema": "^3.23.0"
  }
}
